      <references xsi:type="mutatorenvironment:ReferenceInit">
        <reference href="../model/LogicCircuit.ecore#//OutputPin/tar"/>
        <object xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.2/@commands.3"/>
