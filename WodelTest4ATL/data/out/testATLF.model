    </commands>
    <commands xsi:type="mutatorenvironment:SelectObjectMutator" name="sipe">
      <container xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.0/@commands.0">
        <refType href="../model/ATL.ecore#/0/InPattern/elements"/>
      </container>
      <object xsi:type="mutatorenvironment:RandomTypeSelection">
        <type href="../model/ATL.ecore#/0/SimpleInPatternElement"/>
      </object>
    </commands>
    <commands xsi:type="mutatorenvironment:CreateObjectMutator" name="conj">
      <type href="../model/ATL.ecore#/1/OperatorCallExp"/>
      <attributes xsi:type="mutatorenvironment:AttributeScalar">
        <attribute href="../model/ATL.ecore#/1/OperationCallExp/operationName"/>
        <value xsi:type="mutatorenvironment:SpecificStringType" value="and"/>
      </attributes>
    </commands>
    <commands xsi:type="mutatorenvironment:CreateObjectMutator" name="call">
      <type href="../model/ATL.ecore#/1/OperationCallExp"/>
      <container xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.0/@commands.4">
        <refType href="../model/ATL.ecore#/1/PropertyCallExp/source"/>
      </container>
      <attributes xsi:type="mutatorenvironment:AttributeCopy">
        <attribute href="../model/ATL.ecore#/1/OperationCallExp/operationName"/>
        <attribute href="../model/ATL.ecore#/1/Attribute/name"/>
        <object xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.0/@commands.2"/>
      </attributes>
    </commands>
    <commands xsi:type="mutatorenvironment:CreateObjectMutator" name="exp">
      <type href="../model/ATL.ecore#/1/VariableExp"/>
      <container xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.0/@commands.5">
        <refType href="../model/ATL.ecore#/1/PropertyCallExp/source"/>
      </container>
    </commands>
    <commands xsi:type="mutatorenvironment:ModifyInformationMutator">
      <object xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.0/@commands.3"/>
      <references xsi:type="mutatorenvironment:ReferenceAdd">
        <reference href="../model/ATL.ecore#/1/VariableDeclaration/variableExp"/>
        <object xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.0/@commands.6"/>
      </references>
    </commands>
    <commands xsi:type="mutatorenvironment:ModifyInformationMutator">
      <object xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.0/@commands.4"/>
      <references xsi:type="mutatorenvironment:ReferenceAdd">
