    </commands>
    <commands xsi:type="mutatorenvironment:SelectObjectMutator" name="i0">
      <container xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.2/@commands.2">
        <refType href="../model/LogicCircuit.ecore#//OutputPin/tar"/>
      </container>
      <object xsi:type="mutatorenvironment:RandomTypeSelection">
        <type href="../model/LogicCircuit.ecore#//InputPin"/>
      </object>
    </commands>
    <commands xsi:type="mutatorenvironment:CreateObjectMutator">
      <type href="../model/LogicCircuit.ecore#//OutputPin"/>
      <container xsi:type="mutatorenvironment:SpecificObjectSelection" objSel="//@blocks.2/@commands.1">
        <refType href="../../src/testLC.mutator#|34"/>
